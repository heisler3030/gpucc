  <%= f.error_messages %>
  <div class="form-group">
    <%= f.label :title %>
    <%= f.text_field :title,:class => "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :start_date, "Effective Dates" %>
    <div class="input-daterange input-group" id="datepicker">
      <%= f.text_field :start_date, :class => "input-sm form-control" %>
      <span class="input-group-addon">to</span>
      <%= f.text_field :end_date, :class => "input-sm form-control" %>
    </div>
  </div>

  <div class="form-group" style="padding-top:10px">
    <%= f.label :description %>
    <%= f.text_area :description, :class => "nodisplay summernote" %>
  </div>

  <div class="row">
    <div class="col-sm-4">
      <div class="form-group">
        <%= f.label :max_misses %>
        <%= f.text_field :max_misses,:class => "input-sm form-control" %>
      </div>
    </div>
    <div class="col-sm-4">
      <div class="form-group join-by">
        <%= f.label :join_by %>
        <%= f.text_field :join_by,:class => "input-sm form-control", :data => {:provide => "datepicker", :'date-format' => "yyyy-mm-dd"} %>
      </div>
    </div>
    <div class="col-sm-4">
      <div class="form-group">
        <%= f.label :owner %>
        <%= f.collection_select :owner_id, @owners, :id, :name_with_role, {}, {selected: @owner.id, class: "input-sm form-control" } %>
      </div>
    </div>

  <div class="form-group">
  	<%= f.submit :class => 'btn btn-primary', :style => 'margin-top:10px' %>
  </div>